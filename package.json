    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./button": {
      "require": "./cjs/button/index.cjs",
      "import": "./es/button/index.mjs",
      "types": "./es/button/index.d.ts"
    },
    "./box": {
      "require": "./cjs/box/index.cjs",
      "import": "./es/box/index.mjs",
      "types": "./es/box/index.d.ts"
    },
    "./text": {
      "require": "./cjs/text/index.cjs",
      "import": "./es/text/index.mjs",
      "types": "./es/text/index.d.ts"
    },
    "./separator": {
      "require": "./cjs/separator/index.cjs",
      "import": "./es/separator/index.mjs",
      "types": "./es/separator/index.d.ts"
    },
    "./avatar": {
      "require": "./cjs/avatar/index.cjs",
      "import": "./es/avatar/index.mjs",
      "types": "./es/avatar/index.d.ts"
    },
    "./input": {
      "require": "./cjs/input/index.cjs",
      "import": "./es/input/index.mjs",
      "types": "./es/input/index.d.ts"
    },
    "./description": {
      "require": "./cjs/description/index.cjs",
      "import": "./es/description/index.mjs",
      "types": "./es/description/index.d.ts"
    },
    "./hooks": {
      "require": "./cjs/hooks/index.cjs",
      "import": "./es/hooks/index.mjs",
      "types": "./es/hooks/index.d.ts"
    },
    "./theme": {
      "require": "./cjs/theme/index.cjs",
      "import": "./es/theme/index.mjs",
      "types": "./es/theme/index.d.ts"
    },
    "./preflight": {
      "require": "./cjs/theme/preflight.cjs",
      "import": "./es/theme/preflight.mjs",
      "types": "./es/theme/preflight.d.ts"
    }
  },
  "main": "cjs/index.cjs",
  "module": "es/index.mjs",
  "typings": "es/index.d.ts",
  "typesVersions": {
    "*": {
      "button": [
        "es/button/index.d.ts"
      ],
      "box": [
        "es/box/index.d.ts"
      ],
      "text": [
        "es/text/index.d.ts"
      ],
      "separator": [
        "es/separator/index.d.ts"
      ],
      "avatar": [
        "es/avatar/index.d.ts"
      ],
      "input": [
        "es/input/index.d.ts"
      ],
      "description": [
        "es/description/index.d.ts"
      ],
      "hooks": [
        "es/hooks/index.d.ts"
      ],
      "theme": [
        "es/theme/index.d.ts"
      ],
      "preflight": [
        "es/theme/preflight.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf cjs es && rollup -c --silent",
    "dev": "rollup -c -w --silent",
    "prepare": "husky install",
    "esno": "esno",
    "test": "echo todo",
    "pretest": "pnpm run build",
    "release": "pnpm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "ci:version": "pnpx changeset version",
    "ci:publish": "pnpx changeset publish",
    "ci:snapshot": "pnpx changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpx changeset publish --tag beta",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "dependencies": {
    "@react-spring/web": "^9.4.3",
    "@stitches/react": "^1.2.6",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.5.0",
    "@changesets/cli": "^2.17.0",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jest": "27.0.2",
    "@types/react": "^17.0.39",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.9.0",
    "esno": "^0.14.1",
    "husky": "^7.0.0",
    "jest": "27.2.5",
    "lint-staged": "^11.0.1",
    "prettier": "2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tslib": "^2.3.1",
    "typescript": "4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
